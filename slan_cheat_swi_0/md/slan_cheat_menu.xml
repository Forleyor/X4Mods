				<add_player_choice_sub text="{357423,2002} + '(' + {1025,35} + ')'" section="slan_cheat_modify_station_set" position="bottom_left" choiceparam="['Choice']" comment="Choice" />
				<add_player_choice_return text="{1002,20}" comment="(Back)" />
