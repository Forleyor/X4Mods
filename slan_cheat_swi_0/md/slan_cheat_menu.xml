		<!-- ////  ======================  Add Ship  ======================  \\\\ -->
		<cue name="slan_cheat_add_ship" instantiate="true" namespace="this">
			<conditions>
				<check_any>
					<event_conversation_next_section section="slan_cheat_add_ship" />
