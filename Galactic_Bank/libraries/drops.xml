<diff xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
  xsi:noNamespaceSchemaLocation="../../xsd/diff.xsd">
  <add sel="//droplists">
    <droplist id="ship_medium_banker">
      <drop macro="bank_wares_macro" min="1" max="1" chance="100">
        <collectable>
          <wares selection="random">
            <select weight="1">
              <ware ware="bank_credit_lootbox" min="1" max="4" />
            </select>
          </wares>
        </collectable>
      </drop>
      <drop macro="sm_gen_wares_common_01_macro" min="1" max="2" chance="70">
        <collectable>
          <wares selection="random">
            <select weight="7">
              <ware ware="inv_algaescrubber" min="1" max="3" />
            </select>
            <select weight="7">
              <ware ware="inv_carbonfilter" min="1" max="3" />
            </select>
            <select weight="7">
              <ware ware="inv_crystalbit" min="1" max="3" />
            </select>
            <select weight="12">
              <ware ware="inv_herbicide" min="1" max="3" />
            </select>
            <select weight="10">
              <ware ware="inv_hydraulicpump" min="1" max="3" />
            </select>
            <select weight="5">
              <ware ware="inv_majaglit" min="5" max="15" />
            </select>
            <select weight="10">
              <ware ware="inv_microgimble" min="1" max="3" />
            </select>
            <select weight="10">
              <ware ware="inv_micromotor" min="1" max="3" />
            </select>
            <select weight="8">
              <ware ware="inv_rapidsandfilter" min="1" max="3" />
            </select>
          </wares>
        </collectable>
      </drop>
    </droplist>

  <droplist
      id="ship_large_banker">
      <drop macro="bank_wares_macro" min="1" max="1" chance="100">
        <collectable>
          <wares selection="random">
            <select weight="1">
              <ware ware="bank_credit_lootbox" min="5" max="20" />
            </select>
          </wares>
        </collectable>
      </drop>
      <drop macro="sm_gen_wares_common_01_macro" min="1" max="2" chance="70">
        <collectable>
          <wares selection="random">
            <select weight="7">
              <ware ware="inv_algaescrubber" min="1" max="3" />
            </select>
            <select weight="7">
              <ware ware="inv_carbonfilter" min="1" max="3" />
            </select>
            <select weight="7">
              <ware ware="inv_crystalbit" min="1" max="3" />
            </select>
            <select weight="12">
              <ware ware="inv_herbicide" min="1" max="3" />
            </select>
            <select weight="10">
              <ware ware="inv_hydraulicpump" min="1" max="3" />
            </select>
            <select weight="5">
              <ware ware="inv_majaglit" min="5" max="15" />
            </select>
            <select weight="10">
              <ware ware="inv_microgimble" min="1" max="3" />
            </select>
            <select weight="10">
              <ware ware="inv_micromotor" min="1" max="3" />
            </select>
            <select weight="8">
              <ware ware="inv_rapidsandfilter" min="1" max="3" />
            </select>
          </wares>
        </collectable>
      </drop>
      <!--Common
      Crafting Wares-->
      <drop macro="sm_gen_wares_common_02_macro" min="1" max="1" chance="70">
        <collectable>
          <wares selection="random">
            <select weight="5">
              <ware ware="inv_argnusteak" min="2" max="5" />
            </select>
            <select weight="7">
              <ware ware="inv_bandages" min="1" max="5" />
            </select>
            <select weight="5">
              <ware ware="inv_interfaceunit" min="5" max="15" />
            </select>
            <select weight="5">
              <ware ware="inv_mixedfruit" min="1" max="3" />
            </select>
            <select weight="7">
              <ware ware="inv_needle" min="1" max="3" />
            </select>
            <select weight="7">
              <ware ware="inv_salvagedelectronics" min="1" max="3" />
            </select>
            <select weight="5">
              <ware ware="inv_securecontainer" min="2" max="12" />
            </select>
            <select weight="7">
              <ware ware="inv_sedative" min="1" max="5" />
            </select>
          </wares>
        </collectable>
      </drop>
      <!--Seminars-->
      <drop macro="sm_gen_powerup_wares_01_macro" min="1" max="2" chance="10">
        <collectable>
          <wares selection="random">
            <select weight="30">
              <ware ware="inv_seminar_management_0" min="1" max="3" />
            </select>
            <select weight="15">
              <ware ware="inv_seminar_management_1" min="1" max="2" />
            </select>
            <select weight="5">
              <ware ware="inv_seminar_management_2" min="1" max="1" />
            </select>
            <select weight="30">
              <ware ware="inv_seminar_piloting_0" min="1" max="3" />
            </select>
            <select weight="15">
              <ware ware="inv_seminar_piloting_1" min="1" max="2" />
            </select>
            <select weight="5">
              <ware ware="inv_seminar_piloting_2" min="1" max="1" />
            </select>
          </wares>
        </collectable>
      </drop>
      <!--Common
      Mod Parts-->
      <drop macro="sm_gen_wpsmod_common_01_macro" min="1" max="1" chance="70">
        <collectable>
          <wares selection="random">
            <select weight="1">
              <ware ware="modpart_extendedfuelcontainer" min="1" max="2" />
            </select>
            <select weight="1">
              <ware ware="modpart_highenergycatalyst" min="1" max="2" />
            </select>
            <select weight="1">
              <ware ware="modpart_nividiumcrystallite" min="1" max="2" />
            </select>
            <select weight="1">
              <ware ware="modpart_nividiumoxide" min="1" max="2" />
            </select>
            <select weight="1">
              <ware ware="modpart_tuningsoftware" min="1" max="2" />
            </select>
          </wares>
        </collectable>
      </drop>
      <!--Rare
      Mod Parts T1-->
      <drop macro="sm_gen_wpsmod_rare_01_macro" min="1" max="1" chance="10">
        <collectable>
          <wares selection="random">
            <select weight="1">
              <ware ware="modpart_enginefuelinjector_t1" min="1" max="2" />
            </select>
            <select weight="1">
              <ware ware="modpart_shieldgeneratorcoil_t1" min="1" max="2" />
            </select>
            <select weight="1">
              <ware ware="modpart_shipnanoweave_t1" min="1" max="2" />
            </select>
            <select weight="1">
              <ware ware="modpart_weaponchamber_t1" min="1" max="2" />
            </select>
          </wares>
        </collectable>
      </drop>
      <!--Light
      Missiles-->
      <drop macro="sm_gen_ammo_01_macro" min="1" max="2" chance="50">
        <collectable>
          <ammo selection="random">
            <select weight="7" macro="missile_cluster_light_mk1_macro" min="2" max="4" />
            <select weight="12" macro="missile_dumbfire_light_mk1_macro" min="3" max="6" />
            <select weight="8" macro="missile_dumbfire_light_mk2_macro" min="2" max="4" />
            <select weight="7" macro="missile_emp_mk1_macro" min="2" max="4" />
            <select weight="7" macro="missile_guided_light_mk1_macro" min="2" max="4" />
            <select weight="7" macro="missile_heatseeker_light_mk1_macro" min="2" max="4" />
            <select weight="5" macro="missile_smart_light_mk1_macro" min="1" max="2" />
            <select weight="6" macro="missile_swarm_light_mk1_macro" min="1" max="3" />
          </ammo>
        </collectable>
      </drop>
      <!--Heavy
      Missiles-->
      <drop macro="sm_gen_ammo_02_macro" min="1" max="2" chance="20">
        <collectable>
          <ammo selection="random">
            <select weight="8" macro="missile_cluster_heavy_mk1_macro" min="1" max="2" />
            <select weight="15" macro="missile_dumbfire_heavy_mk1_macro" min="1" max="2" />
            <select weight="8" macro="missile_dumbfire_heavy_mk2_macro" min="1" max="2" />
            <select weight="7" macro="missile_guided_heavy_mk1_macro" min="1" max="2" />
            <select weight="6" macro="missile_heatseeker_heavy_mk1_macro" min="1" max="2" />
            <select weight="5" macro="missile_smart_heavy_mk1_macro" min="1" max="2" />
            <select weight="7" macro="missile_swarm_heavy_mk1_macro" min="1" max="2" />
          </ammo>
        </collectable>
      </drop>
    </droplist>
  </add>
</diff>
