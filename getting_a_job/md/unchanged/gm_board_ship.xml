<?xml version="1.0" encoding="utf-8"?>
<diff>
	<replace  sel="//library[@name='Start']/params/param[@name='OfferDistance']/@default">350km</replace>
    <replace sel="/mdscript/cues/library[@name='Setup_DetermineEnemyFaction_OfPirates']/actions/do_if/@value">(@$Station and $Station.owner == faction.blacksun) or (@$Faction and $Faction == faction.blacksun)</replace>
    <replace sel="/mdscript/cues/library[@name='Setup_DetermineEnemyFaction_OfPirates']/actions/do_if/set_value/@exact">[faction.ascendancy, faction.newrepublic, faction.galempire, faction.huttpirates].random</replace>
    <replace sel="/mdscript/cues/library[@name='Setup_DetermineEnemyFaction_OfPirates']/actions/do_elseif[@value='(@$Station and $Station.owner == faction.hatikvah) or (@$Faction and $Faction == faction.hatikvah)']/@value">(@$Station and $Station.owner == faction.huttpirates) or (@$Faction and $Faction == faction.huttpirates)</replace>
    <replace sel="/mdscript/cues/library[@name='Setup_DetermineEnemyFaction_OfPirates']/actions/do_elseif[@value='(@$Station and $Station.owner == faction.huttpirates) or (@$Faction and $Faction == faction.huttpirates)']/set_value/@exact">[faction.commerceguild, faction.newrepublic, faction.corporate, faction.blacksun].random</replace>
    <replace sel="/mdscript/cues/library[@name='Setup_DetermineEnemyFaction_OfPirates']/actions/do_elseif[@value='(@$Station and md.$DLCPirateFactions.indexof.{@$Station.owner}) or (@$Faction and md.$DLCPirateFactions.indexof.{@$Faction})']/@value">(@$Station and $Station.owner == faction.valarian) or (@$Faction and $Faction == faction.valarian)</replace>
    <replace sel="/mdscript/cues/library[@name='Setup_DetermineEnemyFaction_OfPirates']/actions/do_elseif[@value='(@$Station and $Station.owner == faction.valarian) or (@$Faction and $Faction == faction.valarian)']/set_value/@exact">[faction.commerceguild, faction.newrepublic, faction.corporate, faction.huttpirates].random</replace>
    <replace sel="/mdscript/cues/library[@name='Setup_DetermineEnemyFaction_AnyoneElse']/actions/set_value[@name='$ValidEnemyFactions']/@exact">[faction.newrepublic, faction.galempire, faction.ascendancy, faction.anoat, faction.corporate, faction.commerceguild, faction.mandodw, faction.blacksun, faction.huttpirates, faction.valarian]</replace>
    <replace sel="/mdscript/cues/library[@name='Setup_DetermineEnemyFaction_AnyoneElse']/actions/do_if[@value='(@$Station and $Station.owner == faction.teladi) or (@$Faction and $Faction == faction.teladi) or (@$Station and $Station.owner == faction.ministry) or (@$Faction and $Faction == faction.ministry)']/@value">(@$Station and $Station.owner == faction.newrepublic) or (@$Faction and $Faction == faction.newrepublic) or (@$Station and $Station.owner == faction.rebelalliance) or (@$Faction and $Faction == faction.rebelalliance)</replace>
    <replace sel="/mdscript/cues/library[@name='Setup_DetermineEnemyFaction_AnyoneElse']/actions/do_if[@value='(@$Station and $Station.owner == faction.newrepublic) or (@$Faction and $Faction == faction.newrepublic) or (@$Station and $Station.owner == faction.rebelalliance) or (@$Faction and $Faction == faction.rebelalliance)']/remove_from_list[@name='$ValidEnemyFactions'][@exact='faction.ministry']/@exact">faction.newrepublic</replace>
    <replace sel="/mdscript/cues/library[@name='Setup_DetermineEnemyFaction_AnyoneElse']/actions/do_if[@value='(@$Station and $Station.owner == faction.newrepublic) or (@$Faction and $Faction == faction.newrepublic) or (@$Station and $Station.owner == faction.rebelalliance) or (@$Faction and $Faction == faction.rebelalliance)']/remove_from_list[@name='$ValidEnemyFactions'][@exact='faction.teladi']/@exact">faction.rebelalliance</replace>
    <replace sel="/mdscript/cues/cue[@name='GenerateGenericMission']/cues/cue[@name='GenerateGenericMission_Variant_2']/actions/set_value[@name='$OfferStationFactions']/@exact">[faction.newrepublic, faction.galempire, faction.ascendancy, faction.mandodw, faction.anoat, faction.corporate, faction.commerceguild, faction.huttcartel]</replace>
<!-- kuertee mod: RAP -->
<!--
    <library name="GenerateTextTable">
        <actions>
-->
    <add pos="prepend" sel="//library[@name=&quot;GenerateTextTable&quot;]/actions">
        <include_actions ref="md.kuertee_professions_guilds.GuildMissionContainer_Description" />
    </add>
<!-- ADD:
    <library name="Start">
        <params>
-->
    <add sel="//library[@name=&quot;Start&quot;]/params">
        <param name="MissionGroup" default="null" />
    </add>
<!-- ADD property group:
    <create_offer cue="$MissionCue" ... >
-->
    <add sel="//create_offer[@cue=&quot;$MissionCue&quot;]" type="@group">$MissionGroup</add>
<!--
    <signal_cue_instantly cue="md.GenericMissions.GenericAcceptLogbookEntry" param="[$TextTable.$missionname, $Client, $ClientOwner]"/>
-->
    <add pos="after" sel="//signal_cue_instantly[@cue=&quot;md.GenericMissions.GenericAcceptLogbookEntry&quot;]">
		<set_value name="$DebugChance" exact="md.kuertee_professions_guilds.UserSettings.$debugChance3" />
		<set_value name="namespace.$kProfs_timeStart" exact="player.age" />
    </add>
<!--
    <cue name="MissionEnded">
        <set_value name="stat.missions_completed" operation="add"/>
-->
    <add pos="after" sel="//cue[@name=&quot;MissionEnded&quot;]//set_value[@name=&quot;stat.missions_completed&quot;][@operation=&quot;add&quot;]">
        <set_value name="namespace.$kProfs_isSuccess" exact="true" />
        <set_value name="namespace.$kProfs_rewardCr" exact="$RewardCr" />
        <set_value name="namespace.$kProfs_rewardObj" exact="$RewardObj" />
    </add>
<!--
    <cue name="CleanUp">
        <actions>
-->
    <add pos="prepend" sel="//cue[@name=&quot;CleanUp&quot;]/actions">
        <do_if value="@namespace.$kProfs_timeStart">
            <signal_cue_instantly cue="md.kuertee_professions.GMMissionEnd" param="table[
                $profRepu = md.kuertee_professions.kProfs.$repu_Defender,
                $activityId = if @namespace.$kProfs_isSuccess then md.kuertee_professions.kProfs.$activity_full_mission_success else md.kuertee_professions.kProfs.$activity_full_mission_failure,
                $missionLib = namespace,
                $rewardCr = @namespace.$kProfs_rewardCr,
                $rewardObj = @namespace.$kProfs_rewardObj
            ]" />
        </do_if>
    </add>
</diff>
